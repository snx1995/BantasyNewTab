{
    "manifest_version": 2,
    "name": "bantasy new tab",
    "version": "1.2.1",
    "description": "bantasy new tab",

    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "64": "logo.png",
        "128": "logo.png"
    }

}